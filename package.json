    "@testing-library/user-event": "13.5.0",
    "axios": "0.26.0",
    "camelize-ts": "2.1.0",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "6.2.2",
    "react-scripts": "5.0.0",
    "redux-saga": "1.1.3",
    "reselect": "4.1.5",
    "snakecase-keys": "5.1.2",
    "styled-components": "5.3.3",
    "typescript": "4.6.2",
